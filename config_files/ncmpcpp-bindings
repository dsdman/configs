def_key "l"
  next_column
def_key "h"
  previous_column
def_key "k"
  scroll_up
def_key "j"
  scroll_down
def_key "0"
  move_home
def_key "$"
  move_end
def_key "x"
  delete_playlist_items
def_key "ctrl-v"
  select_range
def_key "v"
  reverse_selection
def_key "V"
  remove_selection
def_key "C"
  toggle_crossfade
def_key "K"
  select_item
  scroll_up
def_key "J"
  select_item
  scroll_down
def_key "ctrl-space"
  select_item
def_key "ctrl-l"
  show_lyrics
